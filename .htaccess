AddDefaultCharset utf8

RewriteEngine On

php_value upload_max_filesize 20G
php_value post_max_size 20G
LimitRequestBody 0

<IfModule mod_security.c>
    SecFilterEngine Off
    SecFilterScanPOST Off
</IfModule>

<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteRule ^(.*)$ public/$1 [L]
</IfModule>
